{% extends 'base.html.twig' %}

{% block title %}Gerant{% endblock %}

{% block contenu %}
    <h1>Gerant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ gerant.id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ gerant.username }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ gerant.roles ? gerant.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ gerant.password }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ gerant.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ gerant.prenom }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ gerant.contact }}</td>
            </tr>
            <tr>
                <th>Creer_le</th>
                <td>{{ gerant.creerLe ? gerant.creerLe|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Creer_par</th>
                <td>{{ gerant.creerPar }}</td>
            </tr>
            <tr>
                <th>Modifie_le</th>
                <td>{{ gerant.modifieLe ? gerant.modifieLe|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Modifie_par</th>
                <td>{{ gerant.modifiePar }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_gerant_index') }}">back to list</a>

    <a href="{{ path('app_gerant_edit', {'id': gerant.id}) }}">edit</a>

    {{ include('gerant/_delete_form.html.twig') }}
{% endblock %}
